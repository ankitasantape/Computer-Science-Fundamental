<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context" xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">
    <bean id="test1" class="com.acme.spring_core_03.model.Test">
        <property name="testId" value="T001"/>
        <property name="testTitle" value="Core Java"/>
        <property name="testMarks" value="80"/>
    </bean>
    <bean id="test2" class="com.acme.spring_core_03.model.Test">
        <property name="testId" value="T002"/>
        <property name="testTitle" value="Oracle"/>
        <property name="testMarks" value="83"/>
    </bean>
    <bean id="student1" class="com.acme.spring_core_03.model.Student">
        <property name="studentId" value="S001"/>
        <property name="studentName" value="Shubham Chaudhary"/>
        <property name="studentTest" ref="test1"/>
    </bean>
    <bean id="student2" class="com.acme.spring_core_03.model.Student">
        <property name="studentId" value="S002"/>
        <property name="studentName" value="skchaudhari"/>
        <property name="studentTest" ref="test2"/>
    </bean>
</beans>